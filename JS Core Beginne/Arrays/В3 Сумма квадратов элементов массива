/*Напишите функцию, которая принимает массив с числами
и находит сумму квадратов элементов этого массива.*/

function calSumSquaresElmArray(arr){
let sum = 0;

    for (let i = 0; i < arr.length; i++) {
        arr[i]= Math.pow(arr[i], 2);
        sum += arr[i];
    }
    return (sum);//14
}
let arr= [1,2,3]
